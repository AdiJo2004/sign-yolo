{
  "version": 2,
  "builds": [
    {
      "src": "application.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "50mb" 
        "includeFiles": ["best.onnx"]  
      }
    },
    {
      "src": "detect.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "application.py"
    }
  ]
}
